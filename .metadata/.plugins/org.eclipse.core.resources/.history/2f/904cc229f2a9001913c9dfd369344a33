schema{
query : Query
}

type Query{
 findByLatLong(longitude,latitude : String): LocationEntry
}

type LocationEntry{

 longitude : String
 latitude : String
}


schema{
query: Query
}

type Query{

getAllLocation : [PolyLocation]
findByLatLong(location: GeoJsonPolygon) : PolyLocation
}

type PolyLocation{

id: String
name: String
location: GeoJsonPolygon
}